---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>mikegriffin.ie</title>
 <link href="http://mikegriffin.ie/feed/" rel="self"/>
 <link href="http://mikegriffin.ie/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://mikegriffin.ie/</id>
 <author>
   <name>Mike Griffin</name>
   <email>iam@mikegriffin.ie</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://mikegriffin.ie{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://mikegriffin.ie{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
